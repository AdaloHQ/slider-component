{
	"displayName": "Range Slider",
	"defaultWidth": 160,
  "defaultHeight": 24,
  "resizeY": true,
  "components": "./index.js", 
	"props": [
    {
      "name": "minValue",
      "displayName": "Minimum Value",
      "type": "number",
      "default": 1
    },
    {
      "name": "maxValue",
      "displayName": "Maximum Value",
      "type": "number",
      "default": 10
    },
    {
      "name": "incrementSize",
      "displayName": "Increment Size",
      "type": "number",
      "default": 1
    },
    {
      "name": "controlledValue",
      "displayName": "Value",
      "type": "number",
      "role": "autosaveInput"
    }
	],
	"childComponents": [
	  {
      "name": "track",
      "displayName": "Track",
      "props": [
        {
          "name": "filledColor",
          "displayName": "Filled Color",
          "type": "color",
          "default": "@primary"
        },
        {
          "name": "unfilledColor",
          "displayName": "Unfilled Color",
          "type": "color",
          "default": "#000"
        },
        {
          "name": "height",
          "displayName": "Height",
          "type": "number",
          "default": 2
        }
	  ]
	  },
	  {
	"name": "marker",
	"displayName": "Marker",
	"props": [
		{
		  "name": "markerColor",
		  "displayName": "Marker Color",
		  "type": "color",
		  "default": "#fff"
		},
		{
      "name": "markerSize",
      "displayName": "Marker Size",
      "type": "number",
      "default": 30
    },
    {
      "name": "markerBorder",
      "displayName": "Border",
      "type": "boolean",
      "default": true
    },
    {
		  "name": "markerBorderColor",
		  "displayName": "Color",
		  "type": "color",
		  "default": "#ddd"
    },
    {
      "name": "markerBorderNum",
      "displayName": " ",
		  "type": "number",
		  "default": 1
	  },
    {
      "name": "shadow",
      "displayName": "Shadow",
      "type": "boolean",
      "default": true
    }
	]
	},
	{
	"name": "labels",
	"displayName": "Labels",
	"props": [
		{
		  "name": "enabled",
		  "type": "boolean",
		  "default": false
		},
		{
		  "name": "bgColor",
		  "displayName": "Background Color",
		  "type": "color",
		  "default": "#fff"
    },
    {
		  "name": "txtColor",
		  "displayName": "Text Color",
		  "type": "color",
		  "default": "@primary"
		},
		{
      "name": "font",
      "displayName": "Font Size",
      "type": "number",
      "default": 18
    }
	]
	}
  ]
}